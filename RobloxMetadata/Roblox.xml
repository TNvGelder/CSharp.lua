<?xml version="1.0" encoding="utf-8"?>
<meta>
  <assembly>
    <namespace name="Roblox">
      <!-- ============================================================ -->
      <!-- InstanceFactory - creates new instances -->
      <!-- ============================================================ -->
      <class name="InstanceFactory">
        <method name="Create" GenericArgCount="1" ArgCount="0" Template="Instance.new({`0Name})" />
        <method name="Create" GenericArgCount="1" ArgCount="1" Template="Instance.new({`0Name}, {0})" />
      </class>

      <!-- ============================================================ -->
      <!-- Globals -->
      <!-- ============================================================ -->
      <class name="Globals">
        <property name="game"><get Template="game" /></property>
        <property name="workspace"><get Template="workspace" /></property>
        <property name="script"><get Template="script" /></property>
      </class>

      <!-- ============================================================ -->
      <!-- EnumItem base class - for enum values -->
      <!-- NOTE: Field access is handled by transpiler (IsRobloxEnumItemType check), -->
      <!-- but metadata is kept as documentation and fallback -->
      <!-- ============================================================ -->
      <class name="EnumItem">
        <property name="Name" IsField="true">
          <get Template="{this}.Name" />
        </property>
        <property name="Value" IsField="true">
          <get Template="{this}.Value" />
        </property>
      </class>

      <!-- ============================================================ -->
      <!-- Instance base interface -->
      <!-- ============================================================ -->
      <class name="Instance">
        <property name="Name" IsField="true">
          <get Template="{this}.Name" />
        </property>
        <property name="Parent" IsField="true">
          <get Template="{this}.Parent" />
        </property>
        <property name="ClassName" IsField="true">
          <get Template="{this}.ClassName" />
        </property>

        <!-- FindFirstChild -->
        <method name="FindFirstChild" ArgCount="1" Template="{this}:FindFirstChild({0})" />
        <method name="FindFirstChild" ArgCount="2" Template="{this}:FindFirstChild({0}, {1})" />
        <method name="FindFirstChild" GenericArgCount="1" ArgCount="1" Template="{this}:FindFirstChild({0})" />
        <method name="FindFirstChild" GenericArgCount="1" ArgCount="2" Template="{this}:FindFirstChild({0}, {1})" />

        <!-- FindFirstChildOfClass -->
        <method name="FindFirstChildOfClass" ArgCount="1" Template="{this}:FindFirstChildOfClass({0})" />
        <method name="FindFirstChildOfClass" GenericArgCount="1" ArgCount="0" Template="{this}:FindFirstChildOfClass({`0Name})" />

        <!-- FindFirstChildWhichIsA -->
        <method name="FindFirstChildWhichIsA" ArgCount="1" Template="{this}:FindFirstChildWhichIsA({0})" />
        <method name="FindFirstChildWhichIsA" GenericArgCount="1" ArgCount="0" Template="{this}:FindFirstChildWhichIsA({`0Name})" />

        <!-- FindFirstAncestor -->
        <method name="FindFirstAncestor" ArgCount="1" Template="{this}:FindFirstAncestor({0})" />
        <method name="FindFirstAncestorOfClass" ArgCount="1" Template="{this}:FindFirstAncestorOfClass({0})" />
        <method name="FindFirstAncestorWhichIsA" ArgCount="1" Template="{this}:FindFirstAncestorWhichIsA({0})" />

        <!-- Hierarchy -->
        <method name="GetChildren" Template="{this}:GetChildren()" />
        <method name="GetDescendants" Template="{this}:GetDescendants()" />
        <method name="Destroy" Template="{this}:Destroy()" />
        <method name="Clone" Template="{this}:Clone()" />

        <!-- IsA -->
        <method name="IsA" ArgCount="1" Template="{this}:IsA({0})" />
        <method name="IsA" GenericArgCount="1" ArgCount="0" Template="{this}:IsA({`0Name})" />

        <method name="IsAncestorOf" ArgCount="1" Template="{this}:IsAncestorOf({0})" />
        <method name="IsDescendantOf" ArgCount="1" Template="{this}:IsDescendantOf({0})" />

        <!-- WaitForChild -->
        <method name="WaitForChild" ArgCount="1" Template="{this}:WaitForChild({0})" />
        <method name="WaitForChild" ArgCount="2" Template="{this}:WaitForChild({0}, {1})" />

        <!-- Attributes -->
        <method name="GetAttribute" ArgCount="1" Template="{this}:GetAttribute({0})" />
